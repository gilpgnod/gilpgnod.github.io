{
 "compilerOptions": {
  "checkJs": true,
  "strictNullChecks": true,
  "target": "ES6",
  "module": "Node16",
  "moduleResolution": "Node16",
  "lib": [
   "ES2017",
   "WebWorker",
   "DOM"
  ],
  "baseUrl": ".",
  "paths": {
   "/*": [
    "./*"
   ]
  }
 },
 "include": [
  "./**/*"
 ],
 "exclude": [
  "node_modules",
  "docs"
 ]
}